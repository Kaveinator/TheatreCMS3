{
    using (Html.BeginForm("EasyLogin", "Account", new { area = "", ReturnUrl = "/Rent/RentalHistories/Index" }, FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        <div class="row ">
            <button type="submit" class="btn cms-bg-main text-white col-2 fixed-bottom">Login as History Manager</button>
        </div>

    }
}